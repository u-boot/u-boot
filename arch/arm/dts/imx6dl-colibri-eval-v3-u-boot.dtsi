#include "imx6qdl-u-boot.dtsi"

&{/aliases} {
	/* U-Boot won't find PMIC otherwise */
	i2c0 = &i2c3;
	i2c1 = &i2c2;
	/* SDHCI instance order: eMMC, 4-bit SD/MMC (U-Boot won't find ConfigBlock otherwise) */
	mmc0 = &usdhc3;
	mmc1 = &usdhc1;
};

&wdog1 {
	status = "okay";
	u-boot,dm-spl;
};
