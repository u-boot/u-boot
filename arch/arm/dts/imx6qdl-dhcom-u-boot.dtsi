/ {
	aliases {
		eeprom0 = &eeprom0;
	};
};

&fec {
	phy-reset-duration = <1>;
	phy-reset-post-delay = <10>;
	phy-supply = <&reg_eth_vio>;
};

&i2c3 {
	eeprom0: eeprom@50 {
	};
};

&reg_usb_otg_vbus {
	gpio = <&gpio3 31 GPIO_ACTIVE_HIGH>;
	enable-active-high;
};
