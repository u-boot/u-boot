	u-boot,dm-spl;
};

&semc {
	u-boot,dm-spl;

	bank1: bank@0 {
		u-boot,dm-spl;
	};
};

&iomuxc {
	u-boot,dm-spl;

	imxrt1020-evk {
		u-boot,dm-spl;

		pinctrl_semc: semcgrp {
			u-boot,dm-spl;
		};

		pinctrl_usdhc0: usdhc0grp {
			u-boot,dm-spl;
		};
	};
};

&pinctrl_lpuart1 {
	u-boot,dm-spl;
};

&usdhc1 {
	u-boot,dm-spl;
};
