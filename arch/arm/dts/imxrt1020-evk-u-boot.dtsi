/ {
	chosen {
		u-boot,dm-spl;
	};

	clocks {
		u-boot,dm-spl;
	};

	soc {
		u-boot,dm-spl;
	};
};

&osc {
	u-boot,dm-spl;
};

&anatop {
	u-boot,dm-spl;
};

&clks {
	u-boot,dm-spl;
};

&gpio1 {
	u-boot,dm-spl;
};

&gpio2 {
	u-boot,dm-spl;
};

&gpio3 {
	u-boot,dm-spl;
};

&gpio5 {
	u-boot,dm-spl;
};

