&binman {
	u-boot_unsigned {
		insert-template = <&u_boot_unsigned_template>;

		fit {
			images {
				uboot {
					description = "U-Boot for AM62Ax board";
				};

				fdt-0 {
					description = "k3-am62a7-phyboard-lyra-rdk";
