/ {
	soc {
		u-boot,dm-pre-reloc;
	};
};

&canvas {
	status = "disabled";
};

&vpu {
	reg = <0x0 0xff900000 0x0 0x100000>,
	      <0x0 0xff63c000 0x0 0x1000>,
	      <0x0 0xff638000 0x0 0x400>;
	reg-names = "vpu", "hhi", "dmc";
	u-boot,dm-pre-reloc;
};

&hdmi_tx {
	reg = <0x0 0x0 0x0 0x10000>,
	      <0x0 0x3c000 0x0 0x1000>;
	reg-names = "hdmitx", "hhi";
};
