#include "rk3326-u-boot.dtsi"

&rk817 {
	regulators {
		vcc_cam: LDO_REG9 {
			regulator-name = "vcc_cam";
			regulator-min-microvolt = <3000000>;
			regulator-max-microvolt = <3000000>;

			regulator-state-mem {
				regulator-off-in-suspend;
				regulator-suspend-microvolt = <3000000>;
			};
		};
	};
};

&saradc {
	bootph-all;
};

&sfc {
	flash@0 {
		bootph-pre-ram;
		bootph-some-ram;
	};
};

&uart1 {
	bootph-all;
	clock-frequency = <24000000>;
};
