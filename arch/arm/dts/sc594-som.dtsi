/dts-v1/;

#include "sc5xx.dtsi"

&timer0 {
	clocks = <&clk ADSP_SC594_CLK_CGU0_SCLK0>;
};

&clk {
	compatible = "adi,sc594-clocks";
	reg = <0x3108d000 0x1000>,
	      <0x3108e000 0x1000>,
	      <0x3108f000 0x1000>;
};
