#include <dt-bindings/input/input.h>
#include "tegra30.dtsi"

/ {
	chosen {
		stdout-path = &uarta;
	};

	aliases {
		i2c0 = &pwr_i2c;

		mmc0 = &sdmmc4;	/* eMMC */

		rtc0 = &pmic;
		rtc1 = "/rtc@7000e000";

		usb0 = &usb1;
	};

	memory {
		device_type = "memory";
		reg = <0x80000000 0x40000000>;
	};

	host1x@50000000 {
		dc@54200000 {
			nvidia,180-rotation;
			rgb {
				status = "okay";

				nvidia,panel = <&panel>;
			};
		};
	};

	gpio@6000d000 {
		volume-buttons-hog {
			gpio-hog;
			gpios = <TEGRA_GPIO(R, 0) GPIO_ACTIVE_HIGH>,
				<TEGRA_GPIO(Q, 0) GPIO_ACTIVE_HIGH>;
			output-low;
		};
	};

