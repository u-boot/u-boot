/dts-v1/;

/ {
	compatible = "xlnx,versal-net-mini";
	#address-cells = <2>;
	#size-cells = <2>;
	model = "Xilinx Versal NET MINI eMMC";

	aliases {
		serial0 = &dcc;
		mmc0 = &sdhci1;
	};

	chosen {
		stdout-path = "serial0:115200";
	};

	memory@0 {
		device_type = "memory";
		reg = <0 0 0 0x20000000>;
	};

	clk200: clk200 {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <200000000>;
	};

	dcc: dcc {
		compatible = "arm,dcc";
		status = "okay";
		bootph-all;
	};

	amba: axi {
		bootph-all;
		compatible = "simple-bus";
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		sdhci1: mmc@f1050000 {
			compatible = "xlnx,versal-net-emmc";
			status = "okay";
			non-removable;
			disable-wp;
			no-sd;
			no-sdio;
			cap-mmc-hw-reset;
			bus-width = <8>;
			reg = <0 0xf1050000 0 0x10000>;
			clock-names = "clk_xin", "clk_ahb";
			clocks = <&clk200>, <&clk200>;
			xlnx,mio-bank = <0>;
		};
	};
};
