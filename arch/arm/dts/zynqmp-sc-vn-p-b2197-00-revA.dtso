	/* EXT_PMBUS main - J10 - disable extended PMBUS */
	vccint: tps53681@60 { /* u1712 - J1770 reset jumper */
		compatible = "ti,tps53681", "ti,tps53679";
		reg = <0x60>;
		/* vccint, vcc_cpm5n */
