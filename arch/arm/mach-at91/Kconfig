config ATMEL_SFR
	bool

config AT91RESET_EXTRST
	bool

config SYS_SOC
	default "at91"

config AT91_GPIO_PULLUP
	bool "Keep pullups on peripheral pins"
	depends on CPU_ARM926EJS

config ATMEL_LEGACY
	bool "Legacy GPIO support"

config AT91_WANTS_COMMON_PHY
	bool

