#include "binman.dtsi"
#include "jh7110-u-boot.dtsi"
/ {
	chosen {
		bootph-pre-ram;
	};

	firmware {
		spi0 = &qspi;
		bootph-pre-ram;
	};

	config {
		bootph-pre-ram;
		u-boot,spl-payload-offset = <0x100000>;
	};

	memory@40000000 {
		bootph-pre-ram;
	};
};

&uart0 {
	bootph-pre-ram;
};

&mmc0 {
	bootph-pre-ram;
};

&mmc1 {
	bootph-pre-ram;
};

&qspi {
	bootph-pre-ram;

	nor-flash@0 {
		bootph-pre-ram;
	};
};

&sysgpio {
	bootph-pre-ram;
};

&mmc0_pins {
	bootph-pre-ram;
	mmc0-pins-rest {
		bootph-pre-ram;
	};
};

&mmc1_pins {
	bootph-pre-ram;
	mmc1-pins0 {
		bootph-pre-ram;
	};

	mmc1-pins1 {
		bootph-pre-ram;
	};
};

