config CLK_RCAR_GEN3
	bool "Renesas RCar Gen3 and Gen4 clock driver"
	def_bool y if RCAR_64
	depends on CLK_RENESAS
	select CLK_RCAR
	select CLK_RCAR_CPG_LIB
	select DM_RESET
	help
	  Enable this to support the clocks on Renesas RCar Gen3 and Gen4 SoCs.

config CLK_R8A774A1
