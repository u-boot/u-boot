/dts-v1/;

/ {
	#address-cells = <1>;
	#size-cells = <1>;

	binman {
		sort-by-offset;
		end-at-4gb;
		size = <0x800000>;
		intel-descriptor {
			filename = "descriptor.bin";
		};

		intel-ifwi {
			offset-unset;
			filename = "ifwi.bin";

			u-boot-tpl {
				replace;
				ifwi-subpart = "IBBP";
				ifwi-entry = "IBBL";
			};
		};
	};
};
